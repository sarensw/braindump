{
  "name": "braindump",
  "productName": "braindump",
  "version": "0.0.1",
  "description": "Dump your brain",
  "main": "main.js",
  "scripts": {
    "start": "snowpack dev --polyfill-node",
    "build": "snowpack build",
    "dev": "concurrently --names \"snowpack,electron\" \"npm run snowpack-dev\" \"npm run electron-start\"",
    "electron-start": "electron .",
    "electron-prod": "cross-env NODE_ENV=production electron .",
    "snowpack-dev": "snowpack dev --polyfill-node --open none"
  },
  "homepage": "./",
  "author": "Stephan Arenswald",
  "license": "UNLICENSED",
  "devDependencies": {
    "@redux-devtools/core": "^3.9.0",
    "@snowpack/plugin-postcss": "^1.4.1",
    "@snowpack/plugin-react-refresh": "^2.5.0",
    "autoprefixer": "^10.2.6",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "electron": "^13.1.2",
    "postcss": "^8.3.4",
    "postcss-cli": "^8.3.1",
    "snowpack": "^3.5.7",
    "standard": "^16.0.3",
    "tailwindcss": "^2.1.4"
  },
  "dependencies": {
    "@codemirror/commands": "^0.18.3",
    "@codemirror/comment": "^0.18.1",
    "@codemirror/fold": "^0.18.1",
    "@codemirror/highlight": "^0.18.4",
    "@codemirror/history": "^0.18.1",
    "@codemirror/language": "^0.18.2",
    "@codemirror/lint": "^0.18.4",
    "@codemirror/search": "^0.18.4",
    "@codemirror/state": "^0.18.7",
    "@codemirror/theme-one-dark": "^0.18.1",
    "@reduxjs/toolkit": "^1.6.0",
    "codemirror": "^5.61.1",
    "dateformat": "^4.5.1",
    "lezer": "^0.13.5",
    "pouchdb-browser": "^7.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "use-pouchdb": "^1.2.3"
  }
}
